          <script src="../EIAnalytics-Gadget-commons/js/jquery-1.10.2.js"></script>
          <script src="../EIAnalytics-Gadget-commons/js/moment.min.js"></script>
          <script src="../EIAnalytics-Gadget-commons/js/gadget-utils.js"></script>

          <!-- D3 Gagre -->
          <script src="../EIAnalytics-Gadget-commons/js/d3.min.js" charset="utf-8"></script>
          <script src="../EIAnalytics-Gadget-commons/js/dagre-d3.js"></script>

