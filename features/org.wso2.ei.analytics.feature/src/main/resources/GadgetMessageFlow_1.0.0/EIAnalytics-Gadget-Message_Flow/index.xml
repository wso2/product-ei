          <link rel="stylesheet" type="text/css" href="/portal/libs/nanoscroller_0.8.7/nanoscroller.css" />
          <script src="/portal/libs/nanoscroller_0.8.7/jquery.nanoscroller.min.js"></script>

          <!-- Custom -->
          <link rel="stylesheet" type="text/css" href="css/custom.css" />
          <script src="js/gadgetconf.js"></script>
          <script src="js/main.js"></script>

        </head>
        <body>

            <div class="nano">
                <div class="nano-content">
                    <div class="page-content-wrapper">
                        <button class="btn btn-primary btn-lg" id="btnViewToggle"><i class="icon fw fw-expand"></i> Maximize View</button>
                        <div id="canvas"></div>
                        <svg id="svg-canvas" width="100%" height="400"></svg>
                    </div>
                </div>
            </div>

        </body>
        ]]>
    </Content>
