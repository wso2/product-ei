	<!-- Get Binding -->
	<wsdl:binding name="UserServiceGetHTTP" type="tns:UserServiceGetPT">
		<http:binding verb="GET" />
		<wsdl:operation name="getUserName">
			<http:operation
				location="http://localhost:9765/UserService_1.0.0/services/user_service/userservice/users/name/{uid}" />
			<wsdl:input>
				<http:urlReplacement />
			</wsdl:input>
			<wsdl:output>
