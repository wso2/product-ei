<messageStore class="org.apache.synapse.message.store.impl.jdbc.JDBCMessageStore"
              xmlns="http://ws.apache.org/ns/synapse"
              name="EmptyMsgBodyTestStore">
    <parameter name="store.jdbc.driver">org.h2.Driver</parameter>
    <parameter name="store.producer.guaranteed.delivery.enable">false</parameter>
    <parameter name="store.jdbc.username">wso2carbon</parameter>
    <parameter name="store.jdbc.password">wso2carbon</parameter>
    <parameter name="store.jdbc.connection.url">$url</parameter>
    <parameter name="store.jdbc.table">JDBC_MESSAGE_STORE</parameter>
</messageStore>
