<definitions xmlns="http://ws.apache.org/ns/synapse">
   <proxy name="ETagTestProxy"
          transports="https http"
          startOnLoad="true"
          trace="disable">
      <description/>
      <target>
         <endpoint>
            <address uri="http://localhost:8280/services/echo"/>
         </endpoint>
         <outSequence>
            <property name="HTTP_ETAG" value="true" scope="axis2" type="BOOLEAN"/>
            <send/>
         </outSequence>
      </target>
   </proxy>
</definitions>
