<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://ws.apache.org/ns/synapse">
   <registry provider="org.wso2.carbon.mediation.registry.WSO2Registry"/>
   <proxy name="testGovRegistry" startOnLoad="true" trace="disable">
      <target>
         <inSequence>
            <log level="full"/>
            <send>
               <endpoint key="gov:/endpointConfig/registry_endpoint.xml"/>
            </send>
         </inSequence>
         <outSequence>
            <send/>
         </outSequence>
      </target>
   </proxy>
   <proxy name="testConfRegistry" startOnLoad="true" trace="disable">
      <target>
         <inSequence>
            <log level="full"/>
            <send>
               <endpoint key="conf:/endpointConfig/registry_endpoint.xml"/>
            </send>
         </inSequence>
         <outSequence>
            <send/>
         </outSequence>
      </target>
   </proxy>
   <proxy name="testLocalRegistry" startOnLoad="true" trace="disable">
      <target>
         <inSequence>
            <log level="full"/>
            <send>
               <endpoint key="local:/endpointConfig/registry_endpoint.xml"/>
            </send>
         </inSequence>
         <outSequence>
            <send/>
         </outSequence>
      </target>
   </proxy>

</definitions>
