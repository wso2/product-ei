<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://ws.apache.org/ns/synapse">
   <proxy name="JMCalloutClientProxy"
          transports="https http"
          startOnLoad="true"
          trace="disable">
      <description/>
      <target>
         <inSequence>
            <property name="FORCE_SC_ACCEPTED" value="true" scope="axis2"/>
            <property name="JMSRequestHeader" value="RequestHeaderVal" scope="transport"/>
            <property name="OUT_ONLY" value="true"/>
            <callout serviceURL="jms:/JMSCalloutBEProxy?transport.jms.ConnectionFactoryJNDIName=QueueConnectionFactory&amp;java.naming.factory.initial=org.apache.activemq.jndi.ActiveMQInitialContextFactory&amp;java.naming.provider.url=tcp://localhost:61616">
               <source xmlns:s12="http://www.w3.org/2003/05/soap-envelope"
                       xmlns:ns="http://org.apache.synapse/xsd"
                       xmlns:s11="http://schemas.xmlsoap.org/soap/envelope/"
                       xpath="s11:Body/child::*[fn:position()=1] | s12:Body/child::*[fn:position()=1]"/>
               <target xmlns:s12="http://www.w3.org/2003/05/soap-envelope"
                       xmlns:ns="http://org.apache.synapse/xsd"
                       xmlns:s11="http://schemas.xmlsoap.org/soap/envelope/"
                       xpath="s11:Body/child::*[fn:position()=1] | s12:Body/child::*[fn:position()=1]"/>
            </callout>
         </inSequence>
      </target>
   </proxy>
   <proxy name="JMSCalloutBEProxy"
          transports="jms"
          startOnLoad="true"
          trace="disable">
      <description/>
      <target>
         <inSequence>
            <log level="custom">
               <property name="JMSRequestHeader" expression="$trp:JMSRequestHeader"/>
            </log>
            <drop/>
         </inSequence>
      </target>
   </proxy>
</definitions>