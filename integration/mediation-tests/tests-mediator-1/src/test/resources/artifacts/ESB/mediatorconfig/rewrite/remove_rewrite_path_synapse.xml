<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://ws.apache.org/ns/synapse">
    <proxy name="urlRewriteProxy" transports="https,http" statistics="disable" trace="disable"
           startOnLoad="true">
        <target>
            <inSequence>
                <log level="full"/>
                <rewrite>
                    <rewriterule>
                        <action type="remove" fragment="path"/>
                    </rewriterule>
                </rewrite>
                <log level="full"/>
                <rewrite>
                    <rewriterule>
                        <action value="/services/SimpleStockQuoteService" type="append"
                                fragment="path"/>
                    </rewriterule>
                </rewrite>
                <log level="full"/>
                <send/>
            </inSequence>
            <outSequence>
                <send/>
            </outSequence>
        </target>
        <description></description>
    </proxy>

</definitions>