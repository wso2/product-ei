<proxy xmlns="http://ws.apache.org/ns/synapse" name="enrichOrderTest" transports="https,http" statistics="disable" trace="disable" startOnLoad="true">
    <target>
        <inSequence>
            <loopback/>
        </inSequence>
        <outSequence>
            <enrich>
                <source type="custom" clone="true" xpath="//orders/order/*"/>
                <target type="custom" xpath="//order/."/>
            </enrich>
            <log level="full"/>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>